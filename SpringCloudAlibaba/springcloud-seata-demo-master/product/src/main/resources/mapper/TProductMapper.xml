<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mcy.product.mapper.TProductMapper">

    <update id="deduct">
        update t_product set count=#{count} where product_id=#{productId}
    </update>
</mapper>
